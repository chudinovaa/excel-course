{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "operator-linebreak": "off",
    "max-len": ["error", { "code": 100 }]
  },
  "extends": ["eslint:recommended", "google"]
}